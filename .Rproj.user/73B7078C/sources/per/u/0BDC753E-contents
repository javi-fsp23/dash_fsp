function crud_table_module(ns_prefix) {
  
  $("#" + ns_prefix + "id_table").on("click", ".delete_btn", function() {
    Shiny.setInputValue(ns_prefix + "id_to_delete", this.id, { priority: "event"});
    $(this).tooltip('hide');
  });
  
  $("#" + ns_prefix + "id_table").on("click", ".edit_btn", function() {
    Shiny.setInputValue(ns_prefix + "id_to_edit", this.id, { priority: "event"});
    $(this).tooltip('hide');
  });
}